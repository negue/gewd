            "jestConfig": "libs/lazy/jest.config.js",
            "tsConfig": "libs/lazy/tsconfig.spec.json",
            "setupFile": "libs/lazy/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "ng-utils": {
      "projectType": "library",
      "root": "libs/ng-utils",
      "sourceRoot": "libs/ng-utils/src",
      "prefix": "",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
